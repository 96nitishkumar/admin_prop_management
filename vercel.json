
{
  "version": 2,
  "builds": [
    {
      "src": "Gemfile",
      "use": "@vercel/ruby"
    },
    {
      "src": "config.ru",
      "use": "@vercel/ruby"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ],
  "env": {
    "RAILS_ENV": "production",
    "RAILS_LOG_TO_STDOUT": "1"
  }
}
